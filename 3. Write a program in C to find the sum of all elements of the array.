#include<stdio.h>

int main()
{   int i, n;
    printf("Input value of n : ");
    scanf("%d",&n);
    int ara[n];
    int sum = 0;

    for(i = 0; i < n; i++){
        printf("element %d : ",i);
        scanf("%d",&ara[i]);
    }


    for(i = 0; i < n; i++){
        sum = sum + ara[i];
    }
    printf("Sum of all elements stored in the array is : %d\n",sum);


    return 0;
}
